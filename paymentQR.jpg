           openGoogleLogin() {
              if(!this.loadingsignup){
                // If a cookie is set in a popup window (as part of an OAuth process, for example), you can detect when this window is closed and then check for the cookie.
                var authWindow = window.open(this.g_authurl, '_blank');

                //THIS CHECKS IF THE OPENED WINDOW WAS CLOSED
                // var checkWindowClosed = setInterval(() => {
                // if (!authWindow || authWindow.closed) {
                //     clearInterval(checkWindowClosed);
                //     // execute method that gets cookie values
                //     this.CheckGoogleAuthentication();
                //   }
                // }, 1000);

                //THIS CHECKS IF WE ARE GETTING A MESSAGE FROM AN OPENED WINDOW
                window.addEventListener('message', (event) => {
                  // Check that the message is coming from your domain
                  if (event.origin === "https://qr.io") {
                    if (event.data === "authentication_complete") {
                      if(!this.google_auth_loading){
                        this.CheckGoogleAuthentication();
                      }
                    }
                  }
                }, false);
              }
            },
            async CheckGoogleAuthentication(){
              this.google_auth_loading = true;
              //hide modal
              const modal = document.getElementById('modal-subscribe');
              modal.classList.remove('show');
              modal.setAttribute('aria-hidden', 'true');
              modal.style.display = 'none';
              const backdrop = document.querySelector('.modal-backdrop');
              if (backdrop) {
                backdrop.parentNode.removeChild(backdrop);
              }
              document.body.classList.remove('modal-open');

              //get cookie values
              function getCookie(name) {
                  let cookies = document.cookie.split(';');
                  for(let i = 0; i < cookies.length; i++) {
                      let cookie = cookies[i].trim();
                      // Check if this cookie string begins with the name we want
                      if (cookie.indexOf(name + '=') === 0) {
                          return cookie.substring(name.length + 1);
                      }
                  }
                  return null;
              }
              this.account = getCookie('account');
              this.verified = getCookie('verified');
              //execute rest only if user logged in
              if(this.account && this.verified){
                const saveDataPromise = this.SaveData();

                // Check if SaveData returned a promise
                if (saveDataPromise && typeof saveDataPromise.then === 'function') {
                    // If it did return a promise, wait for it to resolve
                    saveDataPromise.then(() => {
                        // Set google_auth_loading to false after SaveData completes
                        this.google_auth_loading = false;
                    });
                } else {
                    // If SaveData didn't return a promise, handle it immediately
                    this.google_auth_loading = false;
                }
              }
            }
